	<Style BasedOn="{StaticResource WD.DefaultMenu}" TargetType="{x:Type Menu}" />
	<Style x:Key="WD.DefaultContextMenu" BasedOn="{StaticResource WD.ControlBasicStyle}" TargetType="{x:Type ContextMenu}">
